{
    "extends": [
        "airbnb-base",
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "overrides": [
        {
            "files": [
                "*.test.ts"
            ],
            "plugins": [
                "jest"
            ]
        }
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "@typescript-eslint/indent": ["error", 4]
    }
}
